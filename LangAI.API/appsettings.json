{
  "ConnectionStrings": {
    "Redis": "localhost:6379"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    }
  },
  "OpenAIConfigs": {
    "URL": "https://api.openai.com//v1/chat/completions",
    "GTPModel": "gpt-3.5-turbo",
    "Temperature": 0,
    "MaxTokens": 256,
    "SecretKey": "your-api-key"
  },
  "AllowedHosts": "*"
}